{
    "family": "task-family",
    "executionRoleArn": "arn:aws:iam::255945442255:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "task-family",
            "image": "255945442255.dkr.ecr.ap-southeast-1.amazonaws.com/yuyuan-3.11:latest", 
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
